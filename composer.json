{
  "name": "dasraab/hubspot-laravel",
  "description": "HubSpot API wrapper for Laravel 5.5+.",
  "keywords": [
    "hubspot",
    "laravel",
    "php"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Benjamin Schneider"
    }
  ],
  "require": {
    "php": "^7.0",
    "illuminate/support": "5.7.* || 5.8.* || ^6.0 || ^7.0",
    "hubspot/api-client": "~1.3.0"
  },
  "autoload": {
    "psr-4": {
      "dasraab\\HubspotLaravel\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "dasraab\\HubspotLaravel\\HubspotServiceProvider"
      ],
      "aliases": {
        "HubSpot": "dasraab\\HubspotLaravel\\Facades\\Hubspot"
      }
    }
  }
}
